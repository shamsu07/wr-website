// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PostContactUs {
  id      Int    @id @default(autoincrement())
  name    String
  mobile  String
  email   String
  subject String
  details String
}

model PostPartnerWithUs {
  id       Int    @id @default(autoincrement())
  name     String
  mobile   String
  email    String
  // role     String
  // pincode  Int
  location String
  dob      String
  eduQual  String
}
